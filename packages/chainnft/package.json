{
  "name": "@niftysave/chainnft",
  "version": "0.0.0",
  "description": "Worker that scrapes NFT data from chain",
  "main": "./src/main.js",
  "type": "module",
  "scripts": {
    "test": "mocha *.spec.js",
    "coverage": "c8 npm test && c8 report --reporter lcov --reporter html",
    "lint": "standard",
    "generate-client": "generate-graphql-client --post --endpoint https://api.thegraph.com/subgraphs/name/nftstorage/eip721-subgraph --output ./generated/client && tsc --esModuleInterop --sourceMap generated/client/schema.ts"
  },
  "license": "(Apache-2.0 AND MIT)",
  "devDependencies": {
    "@cloudflare/workers-types": "^2.2.1",
    "@types/assert": "^1.5.4",
    "@types/mocha": "^8.2.2",
    "assert": "^2.0.0",
    "c8": "^7.7.1",
    "cf-workers-memkv": "^1.0.2",
    "mocha": "^8.3.2",
    "standard": "^16.0.3"
  },
  "standard": {
    "ignore": [
      "*.ts"
    ]
  },
  "dependencies": {
    "graphql-typed-client": "1.7.4",
    "multiformats": "^6.0.1",
    "nft.storage": "^0.4.1",
    "typescript": "4.2.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nftstorage/niftysave.git"
  },
  "keywords": [
    "niftysave"
  ],
  "bugs": {
    "url": "https://github.com/nftstorage/niftysave/issues"
  },
  "homepage": "https://github.com/nftstorage/niftysave#readme",
  "publishConfig": {
    "access": "public"
  }
}
