CreateFunction({
  "name": "findResources",
  "body": Query(Lambda(["where", "size", "after", "before"], Let([{
    "status": Select("status", Var("where"))
  }, {
    "match": Match(Index("resource_status"), Var("status"))
  }, {
    "refs": Call("paginate", [Var("match"), {
      "size": Var("size"),
      "after": Var("after"),
      "before": Var("before")
    }])
  }], Map(Var("refs"), Lambda("ref", Get(Var("ref")))))))
})